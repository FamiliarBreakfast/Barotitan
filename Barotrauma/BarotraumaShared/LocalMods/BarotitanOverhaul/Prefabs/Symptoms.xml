        textidentifier="afflictiondescription.bleeding.medium"
        target="Self"
        minstrength="20"
        maxstrength="60"/>
      <Description
        textidentifier="afflictiondescription.bleeding.large"
        target="Self"
        minstrength="60"
        maxstrength="100"/>
    </Affliction>
